    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-focus-lock": "^2.0.5",
    "react-helmet": "^5.2.0",
    "react-intersection-observer": "^8.24.1",
    "react-remove-scroll": "^2.0.4"
  },
  "devDependencies": {
    "eslint-config-react-app": "^4.0.1",
    "gh-pages": "^2.0.1",
    "husky": "^2.4.1",
    "lint-staged": "^8.2.1",
    "prettier": "^1.16.4"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "repository": {
