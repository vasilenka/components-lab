    "@mdx-js/tag": "^0.20.3",
    "@reach/combobox": "^0.1.1",
    "@reach/dialog": "^0.2.9",
    "@reach/observe-rect": "^1.0.3",
    "classnames": "^2.2.6",
    "framer-motion": "^1.6.3",
    "gatsby": "^2.13.45",
    "gatsby-image": "^2.0.37",
    "gatsby-plugin-emotion": "^4.1.2",
    "gatsby-plugin-glamor": "^2.1.3",
    "gatsby-plugin-google-analytics": "^2.0.19",
    "gatsby-plugin-gtag": "^1.0.10",
    "gatsby-plugin-manifest": "^2.0.28",
    "gatsby-plugin-mdx": "^1.0.13",
    "gatsby-plugin-nprogress": "^2.0.10",
    "gatsby-plugin-offline": "^2.0.25",
    "gatsby-plugin-react-helmet": "^3.0.12",
    "gatsby-plugin-react-svg": "^2.1.1",
    "gatsby-plugin-robots-txt": "^1.4.0",
    "gatsby-plugin-sass": "^2.0.11",
    "gatsby-plugin-sharp": "^2.0.32",
    "gatsby-plugin-sitemap": "^2.0.12",
    "gatsby-plugin-webpack-bundle-analyzer": "^1.0.5",
    "gatsby-source-filesystem": "^2.0.29",
    "gatsby-source-prismic-graphql": "^3.3.0",
    "gatsby-transformer-sharp": "^2.1.17",
    "gsap": "^2.1.3",
    "invoker-base": "^1.0.6",
    "invoker-layout": "^1.0.1",
    "invoker-tokens": "^1.0.13",
    "lodash": "^4.17.11",
    "node-sass": "^4.11.0",
    "prism-react-renderer": "^0.1.7",
    "prismic-reactjs": "^1.0.0",
